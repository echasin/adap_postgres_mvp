    <bpmn2:scriptTask id="ScriptTask_1" name="Find The Route" scriptFormat="http://www.java.com/java">
      <bpmn2:extensionElements>
        <tns:metaData name="elementname">
          <tns:metaValue><![CDATA[Find The Route]]></tns:metaValue>
        </tns:metaData>
      </bpmn2:extensionElements>
      <bpmn2:incoming>SequenceFlow_1</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_8</bpmn2:outgoing>
      <bpmn2:script>String filter=(String)kcontext.getVariable(&quot;filterId&quot;);
System.out.println(&quot;In Script Find The Filter ID&quot; + filter);
RouteUtil ru=new RouteUtil ();
WorkFlowRouteExecutor wfe=new WorkFlowRouteExecutor();
ru=wfe.restCall(filter);
System.out.println(&quot;In Script Find The Route Before setvariable :&quot; + ru);

kcontext.setVariable(&quot;routeutil&quot;,ru);
System.out.println(&quot;In Script Find The Route After setvariable :&quot; + ru);</bpmn2:script>
