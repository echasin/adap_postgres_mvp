      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression" id="FormalExpression_4" language="http://www.java.com/java">RouteUtil ruGateway=new RouteUtil ();
ruGateway=(RouteUtil)kcontext.getVariable(&quot;routeutil&quot;);
System.out.println(&quot;Route ID for Business :&quot; +ruGateway.getRouteId());
if(ruGateway.getRouteId() != 0){
System.out.println(&quot;Inside Route ID not equal to 0 in if condition :&quot; +ruGateway);
return true;
}
